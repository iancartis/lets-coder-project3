    "bugs": {
        "url": "https://github.com/iancartis/lets-coder-project3/issues"
    },
    "homepage": "https://github.com/iancartis/lets-coder-project3#readme",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "dotenv": "^8.2.0",
        "email-check": "^1.1.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.11.15",
        "validator": "^13.5.2"
    },
    "devDependencies": {
        "crypto-js": "^4.0.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.7",
