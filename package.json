        "inspect": "node inspect index.js",
        "test": "NODE_ENV=test jest --verbose --silent --detectOpenHandles"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iancartis/lets-coder-project3.git"
    },
    "author": "",
